    "codecov": "^3.1.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "jest": "^23.6.0",
    "nyc": "^13.1.0",
    "react-scripts-ts": "^3.1.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "typescript": "^3.2.2",
    "zen-observable-ts": "^0.8.13"
  }
